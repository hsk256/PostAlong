<?xml version="1.0" encoding="utf-8"?>
<!--toolbar右上角的图标 按下颜色-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/bg_popup_press"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/bg_popup_normal"/>
        </shape>
    </item>
</selector>